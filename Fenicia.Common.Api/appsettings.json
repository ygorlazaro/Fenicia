  "IpRateLimiting": {
    "ClientIdHeader": "X-ClientId",
    "EnableEndpointRateLimiting": true,
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Limit": 5,
        "Period": "10s"
      },
      {
        "Endpoint": "POST:/token/refresh",
        "Limit": 3,
        "Period": "1m"
      }
    ],
    "HttpStatusCode": 429,
    "RealIpHeader": "X-Real-IP",
    "StackBlockedRequests": false
  },
  "IpRateLimitPolicies": {
    "IpRules": [
      {
        "Ip": "127.0.0.1",
        "Rules": [
          {
            "Endpoint": "*",
            "Limit": 10,
            "Period": "10s"
          }
        ]
      }
    ]
